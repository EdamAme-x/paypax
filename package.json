{
  "name": "paypay.x.js",
  "version": "1.0.0",
  "description": "Library for automate PayPay operations",
  "scripts": {
    "format": "prettier --write ./src/*.{ts,tsx} ./src/**/*.{ts,tsx}",
    "lint": "eslint --ext js,ts src",
    "lint:fix": "eslint --ext js,ts src --fix",
    "build": "rimraf dist && tsx ./build.ts",
    "build:deno": "rimraf deno_dist && denoify",
    "build:all": "pnpm build && pnpm build:deno"
  },
  "keywords": [
    "paypay",
    "paypayjs",
    "paypay.js"
  ],
  "author": "ame_x",
  "license": "MIT",
  "packageManager": "pnpm@8.14.1",
  "devDependencies": {
    "@hono/eslint-config": "^0.0.3",
    "denoify": "^1.6.9",
    "esbuild": "^0.15.12",
    "glob": "^8.1.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EdamAme-x/paypay.js.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://paypayjs.ame-x.net",
  "dependencies": {
    "arg": "^5.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
